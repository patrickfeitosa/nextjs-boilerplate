{
  "name": "nextjs-boilerplate",
  "description": "Boilerlate utilizando Typescript, React, NextJS e Styled Components!",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "run-script-os",
    "lint:js:win32": "eslint src/**/*.{js,jsx,ts,tsx} --fix --color",
    "lint:js:darwin:linux": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix --color",
    "lint:js:default": "eslint src/**/*.{js,jsx,ts,tsx} --fix --color",
    "lint:css": "run-script-os",
    "lint:css:win32": "stylelint src/**/*.{js,jsx,ts,tsx}",
    "lint:css:darwin:linux": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:css:default": "stylelint src/**/*.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -p -s./public 6006",
    "build-storybook": "build-storybook -s./public"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "next": "10.0.5",
    "next-pwa": "^5.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/plugin-transform-react-inline-elements": "^7.9.0",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/react": "^6.1.14",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.3",
    "run-script-os": "^1.1.5",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.15.1",
    "npm": ">=6.14.8"
  }
}
